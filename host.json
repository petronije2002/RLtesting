{
  "version": "2.0",
  "extensions": {
    "queues": {
        "maxPollingInterval": "00:00:02",
        "visibilityTimeout" : "00:00:30",
        "batchSize": 1,
        "maxDequeueCount": 1,
        "newBatchThreshold": 0
    }
},



  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[2.*, 3.0.0)"
  }

}
